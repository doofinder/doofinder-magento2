<?xml version="1.0" encoding="UTF-8"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="connect-src">
            <values>
                <value id="doofinder" type="host">*.doofinder.com</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="doofinder_eu1" type="host">eu1-doofinderuser.s3.amazonaws.com</value>
                <value id="doofinder_us1" type="host">us1-doofinderuser.s3.amazonaws.com</value>
            </values>
        </policy>
        <policy id="script-src">
            <values>
                <value id="doofinder" type="host">cdn.doofinder.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
