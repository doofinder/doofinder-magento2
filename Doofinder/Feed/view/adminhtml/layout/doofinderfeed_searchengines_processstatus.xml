<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <referenceBlock name="page.title">
        <action method="setPageTitle">
            <argument name="title" xsi:type="string">Doofinder Index Processing Status</argument>
        </action>
    </referenceBlock>
    <body>
        <referenceContainer name="content">
            <block class="Doofinder\Feed\Block\Adminhtml\SearchEngines\ProcessStatus" name="doofinder-setup" template="Doofinder_Feed::SearchEngines/Process/status.phtml" />
        </referenceContainer>
    </body>
</page>
