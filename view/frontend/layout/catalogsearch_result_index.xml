<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <css src="Doofinder_Feed::css/banner.css" />
    </head>
    <body>
        <referenceContainer name="before.body.end">
            <block class="Doofinder\Feed\Block\Banner" name="doofinder.banner"
                   template="Doofinder_Feed::banner.phtml" />
            <block class="Doofinder\Feed\Block\SearchResult" name="doofinder.search_result"
                   template="Doofinder_Feed::search_result.phtml" />
        </referenceContainer>
    </body>
</page>
